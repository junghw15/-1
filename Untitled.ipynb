    "data=[]\n",
    "i=0\n",
    "\n",
    "while i !=len(a):\n",
    "    \n",
    "    if i<=len(a):\n",
    "        data.append(a[i:i+n])\n",
    "        i=i+n\n",
    "    \n",
    "    else:\n",
    "        data.append(a[i:len(a)])\n",
    "    \n",
    "print(data)"
